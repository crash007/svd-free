{
	"manifest_version": 2,
	"name": "SvD Free",
	"version": "0.21",
	"description": "Unlimited number of free articles on svd.se.",
	"homepage_url": "https://github.com/crash007/svd-free",
	"author": "crash007",
	"icons": 
		{ 
	          "128": "icon128.png",
			  "48": "icon128.png"
		},
	"browser_action": {
		"default_icon": {
     		"32" : "icon128.png"
		}
	},
	"background": {
		"scripts": ["libs/jquery-3.2.1.min.js", "background.js"]
	 	
	},
	"permissions": [
	  "cookies",
	  "*://*.svd.se/*"
	]
}
